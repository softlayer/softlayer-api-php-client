{
    "name": "softlayer/softlayer-api-php-client",
    "description": "SoftLayer API PHP client",
    "keywords": ["softlayer"],
    "homepage": "https://sldn.softlayer.com/php/",
    "license": "MIT",
    "authors": [
        {
            "name": "SoftLayer Development Team",
            "email": "sldn@softlayer.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-soap": "*"
    },
    "autoload": {
        "psr-4": {
            "SoftLayer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SoftLayer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "config": {
            "sort-packages": true
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": ">=3.13.0",
        "phpunit/phpunit": ">=9.0"
    }
}
